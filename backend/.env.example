# Copy this file to .env and fill values (DO NOT commit secrets)

# Stripe keys (test keys recommended for development)
STRIPE_SECRET_KEY=
STRIPE_PUBLISHABLE_KEY=

# Redirects used by Stripe checkout
STRIPE_SUCCESS_URL=http://127.0.0.1:5500/success.html
STRIPE_CANCEL_URL=http://127.0.0.1:5500/premium.html

# Optional JWT secret (backend uses an internal default if not provided)
# JWT_SECRET_KEY=

# Database connection example (if you prefer using env var instead of editing database.py)
# MYSQL_USER=root
# MYSQL_PASSWORD=yourpassword
# MYSQL_HOST=127.0.0.1
# MYSQL_PORT=3306
# MYSQL_DB=medbuddy_db
# SQLALCHEMY_DATABASE_URL=mysql+mysqlconnector://${MYSQL_USER}:${MYSQL_PASSWORD}@${MYSQL_HOST}:${MYSQL_PORT}/${MYSQL_DB}
