# Copy this file to .env in the project root to customize Docker Compose.

# MySQL root-level creds (for the mysql container)
MYSQL_ROOT_PASSWORD=strong_root_password

# Application database and user
MYSQL_DATABASE=medconsult_db
MYSQL_USER=meduser
MYSQL_PASSWORD=medpass

# API container DB connection (host is the docker-compose service name)
API_MYSQL_HOST=mysql
API_MYSQL_PORT=3306


